{
  "presets": [["es2015", { "modules": false }]],
  "plugins": [
    [
      "react-css-modules",
      {
        "generateScopedName": "[path]_[name]_[local]_[hash:base64:5]"
      }
    ],
    "transform-react-jsx",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ]
}
